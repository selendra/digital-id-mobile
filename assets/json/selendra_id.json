{
    "id": "https://koompi.org/person.schema.json",
    "schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Person",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "description": "Your selendra ID",
            "editable": true
        },
        "firstName": {
            "type": "string",
            "description": "The person's first name.",
            "editable": true
        },
        "lastName": {
            "type": "string",
            "description": "The person's last name.",
            "editable": true
        },
        "age": {
            "description": "Age in years which must be equal to or greater than zero.",
            "type": "integer",
            "minimum": 0,
            "editable": true
        },
        "type": {
            "type": "string",
            "description": "Selendra ID",
            "editable": false
        },
        "dob": {
            "type": "string",
            "description": "Your birthday",
            "editable": true
        },
        "gender": {
            "type": "string",
            "description": "Your gender",
            "editable": true
        },
        "address": {
            "type": "string",
            "description": "Your address",
            "editable": true
        }
    }
}